{
  "eslint.enable": true,
  "emmet.includeLanguages": {
    "nunjucks": "html",
    "njk": "html"
  },
  "html-css-class-completion.includeGlobPattern": "**/*.{css,html,njk}",
  "files.associations": {
    "*.njk": "jinja-html"
  },
  "vsicons.associations.files": [{ "icon": "nunjucks", "extensions": ["njk"], "format": "svg" }],
  "files.eol": "\n"
}
